# Base REST API URL (used by axios http client)
VITE_API_BASE_URL=/api

# WebSocket base URL (used by WS client)
# Example: ws://localhost:1234 or wss://your-ws.example.com
VITE_WS_URL=ws://localhost:1234

# Toggle mock mode for REST and WS: "true" or "false"
VITE_USE_MOCKS=true

# App display name in header/topbar
VITE_APP_NAME=Online Test
